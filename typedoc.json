{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/modules"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "theme": "default",
    "plugin": [
        "typedoc-plugin-merge-modules", 
        "typedoc-material-theme"
    ],
    "mergeModulesRenameDefaults": true,
    "mergeModulesMergeMode": "project",
    "themeColor": "#cb9820",
}